    "lint": "eslint src",
    "lint-all": "eslint ."
  },
  "dependencies": {
    "babel-polyfill": "^6.16.0",
    "es6-promise": "3.0.2",
    "babel-types": "^6.24.1",
    "babylon": "^6.17.0",
    "resolve": "1.1.6"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "3.15.0",
    "eslint-plugin-babel": "4.1.1",
    "eslint-plugin-jasmine": "2.2.0"
  },
  "files": [
    "LICENSE",
    "README.md",
    "index.js",
    "lib"
  ],
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "syntax-flow",
      "transform-flow-strip-types"
    ]
  },
  "bugs": {
    "url": "https://github.com/peterhal/sabor/issues"
  },
  "homepage": "https://github.com/peterhal/sabor#readme",
  "keywords": [
    "JavaScript"
